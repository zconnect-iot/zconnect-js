{
  "extends": ["airbnb"],

  "ecmaFeatures": {
    "globalReturn": true,
    "jsx": true,
    "modules": true
  },

  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },

  "globals": {
    "document": false,
    "escape": false,
    "navigator": false,
    "unescape": false,
    "window": false,
    "describe": true,
    "before": true,
    "it": true,
    "expect": true,
    "sinon": true
  },

  "parser": "babel-eslint",

  "plugins": [

  ],

  "rules": {
    "semi": ["error", "never"],
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "jsx-a11y/href-no-hash": 0
  }
}
